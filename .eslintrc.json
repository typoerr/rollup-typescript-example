{
  "extends": [
    "@typoerr/eslint-config",
    "@typoerr/eslint-config/+typescript"
  ],
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9
  },
  "ignorePatterns": [
    "**/public/**",
    "**/dist/**",
    "**/node_modules/**"
  ]
}